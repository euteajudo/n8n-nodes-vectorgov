{
  "name": "n8n-nodes-vectorgov",
  "version": "0.1.6",
  "description": "n8n node para busca semântica em documentos jurídicos brasileiros via VectorGov",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "vectorgov",
    "legal",
    "juridico",
    "rag",
    "search",
    "brasil",
    "licitacao"
  ],
  "license": "MIT",
  "homepage": "https://vectorgov.io",
  "author": {
    "name": "VectorGov",
    "email": "contato@vectorgov.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euteajudo/n8n-nodes-vectorgov.git"
  },
  "bugs": {
    "url": "https://github.com/euteajudo/n8n-nodes-vectorgov/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/VectorGovApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/VectorGov/VectorGov.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
